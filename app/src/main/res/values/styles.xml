<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="roundedCornersImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="color">@android:color/transparent</item>
    </style>
    <style name="roundedAllCornersImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="color">@android:color/transparent</item>
    </style>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="color">@android:color/transparent</item>
        <item name="strokeColor">@color/white</item>
        <item name="strokeWidth">2dp</item>
    </style>
    <style name="TextTabLayout" parent="Base.Widget.Design.TabLayout">
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textStyle">bold</item>
        <item name="tabPaddingEnd">12dp</item>
        <item name="tabTextColor">#80757575</item>
        <item name="tabSelectedTextColor">#757575</item>
    </style>
</resources>